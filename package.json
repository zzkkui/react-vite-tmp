{
  "name": "react-boa-vite",
  "version": "1.0.0",
  "main": "index.js",
  "author": "zzkkui",
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "start": "npm run lint && vite",
    "build": "vite build",
    "analyze": "vite build -- --analyze",
    "serve": "vite preview",
    "lint": "concurrently \"npm run tsc\" \"npm run lint:script \" \"npm run lint:style\"",
    "lint:fix": "concurrently \"npm run lint-fix:script \" \"npm run lint-fix:style\"",
    "lint:script": "eslint --ext .tsx,.ts,.js,.jsx ./src",
    "lint:style": "stylelint \"src/**/*.less\" --custom-syntax postcss-less",
    "lint-fix:script": "npm run lint:script -- --fix",
    "lint-fix:style": "npm run lint:style -- --fix",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "2.0",
    "ahooks": "^3.7.8",
    "antd": "^5.13.1",
    "axios": "^1.6.5",
    "classnames": "^2.5.1",
    "core-js": "^3.35.0",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/qs": "^6.9.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.17",
    "body-parser": "^1.20.2",
    "browserslist": "^4.22.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "click-to-react-component": "^1.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "less": "^4.2.0",
    "lightningcss": "^1.22.1",
    "lint-staged": "^15.2.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.2.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.1.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "unplugin-icons": "^0.18.2",
    "vite": "^5.0.11",
    "vite-aliases": "^0.11.4",
    "vite-plugin-compression2": "^0.11.0",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-imp": "^2.4.0",
    "vite-plugin-mock": "^3.0.1",
    "vite-plugin-purge-icons": "^0.10.0",
    "vite-plugin-restart": "^0.4.0",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint:script",
      "prettier .prettierrc.js --write"
    ],
    "*.{less}": [
      "npm run lint:style"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ie >= 11",
      "chrome >= 49",
      "Firefox ESR",
      "Firefox >= 20"
    ]
  }
}
